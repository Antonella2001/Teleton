@model Dominio.Responsable

@{
    ViewData["Title"] = "Details";
}


<div class="container">
    <h4 class="text-center">Responsable: Detalles</h4>
    <div class="p-3 mb-2 bg-secondary text-white">Nombre: @Html.DisplayFor(model => model.Nombre)</div>
    <div class="p-3 mb-2 bg-secondary text-white">Cédula: @Html.DisplayFor(model => model.Cedula)</div>
    <div class="p-3 mb-2 bg-secondary text-white">Correo: @Html.DisplayFor(model => model.Correo)</div>
    <div class="p-3 mb-2 bg-secondary text-white">Teléfono: @Html.DisplayFor(model => model.Telefono)</div>
    <div class="p-3 mb-2 bg-secondary text-white">Retira: @Html.DisplayFor(model => model.Retira)</div>
    <div class="text-right text-danger">
        <a asp-action="Index">Volver al Listado</a>
    </div>

</div>
